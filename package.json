{
  "name": "space-station",
  "version": "1.0.0",
  "description": "Build a complete web application in this Capstone project. Build all aspects of the site including an interactive front end. On the backend, manage data with a database, talk to APIs, and, host the site on the web so the world can see your finished, running web application.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "postinstall": "cd client && npm install",
    "dev": "concurrently \"nodemon server.js\" \"cd client && npm start\"",
    "heroku-postbuild": "cd client && npm run build",
    "test": "mocha --exit"
  },
